 <nav class="<%=  get_nav_color %>">
  <%= link_to Dashboard, dashboard_root_path , class:"navbar-brand nav-link"%>
  <%# <a  href="#">Navbar w/ text</a> %>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarText" aria-controls="navbarText" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarText">
    <ul class="navbar-nav mr-auto">
      <% if user_signed_in? %>
        <li class="nav-item active">
          <%= link_to 'Profile', user_path,class:"nav-link"%>
        </li>
        <%if current_user.admin?%>
          <%= link_to 'Users', admin_users_path,class:"nav-link"%>
          <%= link_to 'Clients', admin_clients_path,class:"nav-link"%>
          <%= link_to 'Projects', admin_projects_path,class:"nav-link"%>
        <%elsif current_user.manager? %>
          <%= link_to 'Users', manager_users_path,class:"nav-link"%>
          <%= link_to 'Clients', manager_clients_path,class:"nav-link"%>
          <%= link_to 'Projects', manager_projects_path,class:"nav-link"%>
        <%elsif current_user.employee? %>
          <%= link_to 'Clients', employee_clients_path,class:"nav-link"%>
        </li>
        <%= link_to 'Projects', employee_projects_path,class:"nav-link"%>
      <%end%>
    <%end%>
  </ul>
  <span class="navbar-text">
    <% if user_signed_in? %>
      <%= link_to 'Log out', destroy_user_session_path, method: :delete %>
    <%else%>
      <%= link_to 'Sign in', new_user_session_path, class:"nav-link"%>
    <% end %>
  </span>
</div>
</nav>
